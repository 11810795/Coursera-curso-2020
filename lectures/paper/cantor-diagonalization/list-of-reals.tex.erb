\Large
<% terms = 40 %>

<%
srand(3)

def f(x)
  return x+1 if x <= 7
  return x-1 if x >= 8
  throw 'Broken f'
end

digits = []
for i in 1..terms
  digits[i-1] = (1..terms).collect{ |x| rand(10).to_s }.join( '' )
end

missing = (1..terms).collect{ |i| f(digits[i-1][i-1].to_i).to_s }.join( '' )

%>

\begin{align*}
<% for i in 1..30 %>
a_{<%= i %>} &= 0.<%= digits[i-1] %>\cdots \\
<% end %> \\
x &= 0.<%= missing %>\cdots
\end{align*}



